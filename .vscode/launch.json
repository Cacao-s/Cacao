{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Expo Web",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/apps/mobile",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "start"
      ],
      "console": "integratedTerminal",
      "outFiles": [
        "${workspaceFolder}/apps/mobile/.expo/**/*.js"
      ]
    },
    {
      "name": "Expo Android",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/apps/mobile",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "android"
      ],
      "console": "integratedTerminal",
      "outFiles": [
        "${workspaceFolder}/apps/mobile/.expo/**/*.js"
      ]
    },
    {
      "name": "Expo iOS",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/apps/mobile",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "ios"
      ],
      "console": "integratedTerminal",
      "outFiles": [
        "${workspaceFolder}/apps/mobile/.expo/**/*.js"
      ]
    },
    {
      "name": "Attach to Expo",
      "type": "node",
      "request": "attach",
      "port": 19000,
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ],
  "compounds": []
}
